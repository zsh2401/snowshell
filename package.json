{
  "name": "snowshell",
  "version": "0.0.1",
  "sz_rat": {
    "version": "12.0.0",
    "vname": "Venus",
    "description": "Seymour Zhang's React Application Template",
    "repo": "https://github.com/zsh2401/sz-rat"
  },
  "engines": {
    "node": ">= 14"
  },
  "description": "Snow Shell",
  "main": "./main/main.js",
  "scripts": {
    "deploy": "cross-env TS_NODE_PROJECT=\"./tsconfig.webpack.json\" ts-node ./deploy.ts",
    "test": "jest",
    "build": "npm run build:main:prod && npm run build:renderer:prod && yarn electron-packager ./dist snowshell --asar --all --out build",
    "build:renderer:prod": "cross-env TS_NODE_PROJECT=\"./tsconfig.webpack.json\" webpack --config ./webpack.config.renderer.prod.ts",
    "build:renderer:dev": "cross-env TS_NODE_PROJECT=\"./tsconfig.webpack.json\" webpack --config ./webpack.config.renderer.dev.ts",
    "build:main:dev": "cross-env TS_NODE_PROJECT=\"./tsconfig.webpack.json\" webpack --config ./webpack.config.main.dev.ts",
    "build:main:prod": "cross-env TS_NODE_PROJECT=\"./tsconfig.webpack.json\" webpack --config ./webpack.config.main.prod.ts",
    "start:renderer:dev": "cross-env TS_NODE_PROJECT=\"./tsconfig.webpack.json\" webpack serve --config ./webpack.config.dev.ts",
    "start:renderer:prod": "cross-env TS_NODE_PROJECT=\"./tsconfig.webpack.json\" webpack serve --config ./webpack.config.prod.ts",
    "rstart": "cross-env TS_NODE_PROJECT=\"./tsconfig.webpack.json\" webpack serve --config ./webpack.config.renderer.dev.ts",
    "estart": "npm run build:main:dev && npm run eapp",
    "eapp": "electron ./dist/main/main.js",
    "start": "concurrently \"npm run rstart\" \"npm run estart\""
  },
  "keywords": [
    "react",
    "react-router",
    "SPA",
    "PWA"
  ],
  "author": "zsh2401",
  "license": "MIT",
  "devDependencies": {
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/react-hooks": "^6.0.0",
    "@types/connect-history-api-fallback": "^1.3.4",
    "@types/copy-webpack-plugin": "^8.0.0",
    "@types/html-webpack-plugin": "^3.2.5",
    "@types/jest": "^26.0.23",
    "@types/lodash": "^4.14.169",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^17.0.6",
    "@types/react-dom": "^17.0.5",
    "@types/react-loadable": "^5.5.5",
    "@types/react-router-dom": "^5.1.7",
    "@types/webpack": "^5.28.0",
    "@types/webpack-bundle-analyzer": "^4.4.0",
    "@types/workbox-webpack-plugin": "^5.1.6",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^6.1.0",
    "connect-history-api-fallback": "^1.6.0",
    "copy-webpack-plugin": "^8.1.1",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.4",
    "electron": "^12.0.8",
    "electron-packager": "^15.2.0",
    "file-loader": "^6.2.0",
    "gh-pages": "^3.1.0",
    "html-webpack-plugin": "^5.3.1",
    "jest": "^26.6.3",
    "popper.js": "^1.16.1",
    "process": "^0.11.10",
    "style-loader": "^2.0.0",
    "ts-jest": "^26.5.6",
    "ts-loader": "^9.2.0",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.4",
    "url-loader": "^4.1.1",
    "webpack": "5.37.0",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "4",
    "webpack-dev-server": "^3.11.2",
    "webpack-parallel-uglify-plugin": "^2.0.0",
    "webpack-pwa-manifest": "^4.3.0",
    "workbox-webpack-plugin": "^6.1.5"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "nprogress": "^0.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-loading": "^2.0.3",
    "react-router-dom": "^5.2.0",
    "react-use": "^17.2.4",
    "sz-react-support": "^0.0.5"
  }
}
